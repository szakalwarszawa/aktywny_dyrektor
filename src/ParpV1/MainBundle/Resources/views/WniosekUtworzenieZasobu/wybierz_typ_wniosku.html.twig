{% extends '::base.html.twig' %}

{% block body -%}
<script type="text/javascript"src="{{ asset('js/wnioskiZasob.js') }}"></script>
    {{ form_start(form, {'attr' : {'class' : 'container'}}) }}  
    <div class="col-sm-12 container">
        <div class="form-group">
            <label class="col-sm-2  control-label"></label>
            <div class="col-sm-10 pull-right">{{ form_widget(form.dalej, {'attr' : {'onclick' : 'submitWniosekType(event)'}}) }}  </div>
        </div> 
        <table class="table bordered">
            <tr>
                <th rowspan="3" vertical-align="middle" >
                    Typ wniosku
                </th>
                <th>Zgłoszenie nowego zasobu</th>
                <th>Zgłoszenie zmiany</th>
                <th>Wycofanie zasobu</th>
            </tr>
            <tr>
                {% if form.typWnioskuDoRejestru is defined %}<td>{{ form_row(form.typWnioskuDoRejestru) }}</td>{% endif %}
                {% if form.typWnioskuZmianaInformacji is defined %}<td>{{ form_row(form.typWnioskuZmianaInformacji) }}</td>{% endif %}
                {% if form.typWnioskuWycofanie is defined %}<td>{{ form_row(form.typWnioskuWycofanie) }}</td>{% endif %}
            </tr>
            <tr>
                {% if form.typWnioskuDoUruchomienia is defined %}<td>{{ form_row(form.typWnioskuDoUruchomienia) }}</td>{% endif %}
                {% if form.typWnioskuZmianaWistniejacym is defined %}<td>{{ form_row(form.typWnioskuZmianaWistniejacym) }}</td>{% endif %}
                {% if form.typWnioskuWycofanieZinfrastruktury is defined %}<td>{{ form_row(form.typWnioskuWycofanieZinfrastruktury) }}</td>{% endif %}
            </tr>
        </table>
        <div class="form-group">
            <label class="col-sm-2  control-label"></label>
            <div class="col-sm-10 pull-right">{{ form_widget(form.dalej2, {'attr' : {'onclick' : 'submitWniosekType(event)'}}) }}  </div>
        </div> 
    </div>
    <div class="col-sm-12 container">
    
    </div>
    {{ form_end(form, {'render_rest': false}) }}
{% endblock %}