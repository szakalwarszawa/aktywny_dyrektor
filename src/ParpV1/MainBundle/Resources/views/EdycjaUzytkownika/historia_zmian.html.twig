 <div class="tabContent">
    {% if user_changes.aktywne is defined%}
        <table class="table table-hover table-responsive" id="historiaTable">
            <thead>
                <tr>
                    <th>Lp. (id)</th>
                    <th>Imię i nazwisko</th>
                    <th>Inicjały</th>
                    <th>Departament</th>
                    <th>Sekcja</th>
                    <th>Manager</th>
                    <th>Stanowisko</th>
                    <th>Info</th>
                    <th>Uprawnienia początkowe</th>
                    <th>Ad zmiana</th>
                    <th>Od kiedy</th>
                </tr>
            </thead>
            <tbody>
                {% for row in user_changes.aktywne %}
                    <tr>
                        <td>{{ loop.index }} ({{ row.id }})</td>
                        <td>{{ row.cn }}</td>
                        <td>{{ row.initials }}</td>
                        <td>{{ row.department }}</td>
                        <td>{{ row.division }}</td>
                        <td>{{ row.manager }}</td>
                        <td>{{ row.title }}</td>
                        <td>{{ row.info }}</td>
                        <td>{{ row.initialrights }}</td>
                        <td>{{ row.distinguishedName }}</td>
                        <td>{{ row.fromWhen|date('Y-m-d H:i:s') }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    {% else %}
        <p>Użytkownik nie posiada żadnych oczejących zmian.</p>
    {% endif %}
</div>
