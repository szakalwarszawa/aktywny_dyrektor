{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Usuwanie: Wniosek</h1>
    <div class="alert alert-danger">
        Czy na pewno chcesz skasować poniższy Wniosek ?
    </div>
    <table class="record_properties table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Deletedat</th>
                <td>{{ entity.deletedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Createdby</th>
                <td>{{ entity.createdBy }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Lockedby</th>
                <td>{{ entity.lockedBy }}</td>
            </tr>
            <tr>
                <th>Lockedat</th>
                <td>{{ entity.lockedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Numer</th>
                <td>{{ entity.numer }}</td>
            </tr>
            <tr>
                <th>Jednostkaorganizacyjna</th>
                <td>{{ entity.jednostkaOrganizacyjna }}</td>
            </tr>
            <tr>
                <th>Statusname</th>
                <td>{{ entity.statusname }}</td>
            </tr>
            <tr>
                <th>Viewernames</th>
                <td>{{ entity.viewernames }}</td>
            </tr>
            <tr>
                <th>Editornames</th>
                <td>{{ entity.editornames }}</td>
            </tr>
        </tbody>
    </table>

        <div class="btn-group">

    {{ form(delete_form) }}
</div>
{% endblock %}
