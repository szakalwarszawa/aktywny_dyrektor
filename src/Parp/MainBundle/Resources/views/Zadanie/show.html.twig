{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Usuwanie: Zadanie</h1>
    <div class="alert alert-danger">
        Czy na pewno chcesz skasować poniższy Zadanie ?
    </div>
    <table class="record_properties table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Deletedat</th>
                <td>{{ entity.deletedAt|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Nazwa</th>
                <td>{{ entity.nazwa }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Opis</th>
                <td>{{ entity.opis }}</td>
            </tr>
            <tr>
                <th>Komentarz</th>
                <td>{{ entity.komentarz }}</td>
            </tr>
            <tr>
                <th>Osoby</th>
                <td>{{ entity.osoby }}</td>
            </tr>
            <tr>
                <th>Datadodania</th>
                <td>{{ entity.dataDodania|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Dataukonczenia</th>
                <td>{{ entity.dataUkonczenia|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Ukonczoneprzez</th>
                <td>{{ entity.ukonczonePrzez }}</td>
            </tr>
            <tr>
                <th>Obiekt</th>
                <td>{{ entity.obiekt }}</td>
            </tr>
            <tr>
                <th>Obiektid</th>
                <td>{{ entity.obiektId }}</td>
            </tr>
        </tbody>
    </table>

        <div class="btn-group">

    {{ form(delete_form) }}
</div>
{% endblock %}
