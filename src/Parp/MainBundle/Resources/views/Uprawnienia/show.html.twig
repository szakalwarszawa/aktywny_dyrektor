{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Usuwanie: Uprawnienia</h1>
    <div class="alert alert-danger">
        Czy na pewno chcesz skasować poniższy Uprawnienia ?
    </div>
    <table class="record_properties table table-striped">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Opis</th>
                <td>{{ entity.opis }}</td>
            </tr>
            <tr>
                <th>Czy_sekcja</th>
                <td>{{ entity.czysekcja }}</td>
            </tr>
            <tr>
                <th>Czy_edycja</th>
                <td>{{ entity.czyedycja }}</td>
            </tr>
        </tbody>
    </table>

        <div class="btn-group">

    {{ form(delete_form) }}
</div>
{% endblock %}
